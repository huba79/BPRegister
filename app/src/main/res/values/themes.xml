<resources>
    <!-- App base theme. -->
    <style name="Base.Theme.BPRegister" parent="Theme.Material3.Light">
        <!-- Customize your light theme here. -->
        <item name="colorPrimary">@color/material_dynamic_primary10</item>
        <item name="colorPrimaryVariant">@color/material_dynamic_primary30</item>
        <item name="colorOnPrimary">@color/material_dynamic_primary90</item>
        <item name="colorOnPrimaryContainer">@color/material_dynamic_primary50</item>

        <item name="colorSecondary">@color/material_dynamic_secondary10</item>
        <item name="colorSecondaryVariant">@color/material_dynamic_secondary30</item>
        <item name="colorOnSecondary">@color/material_dynamic_secondary90</item>
        <item name="colorSecondaryContainer">@color/material_dynamic_secondary50</item>

        <item name="colorTertiary">@color/material_dynamic_neutral10</item>
        <item name="colorOnTertiary">@color/material_dynamic_neutral80</item>
        <item name="colorOnTertiaryContainer">@color/material_dynamic_neutral_variant30</item>

<!--        <item name="colorSurfaceContainerLowest">@color/material_dynamic_neutral90</item>-->
        <item name="colorSurface">@color/material_dynamic_secondary90</item>
        <item name="colorOnSurface">@color/material_dynamic_secondary10</item>
        <item name="colorSurfaceVariant">@color/material_dynamic_secondary60</item>

        <item name="colorOnContainer">@color/material_dynamic_tertiary70</item>
        <item name="colorContainer">@color/material_dynamic_tertiary30</item>

        <item name="colorBackgroundFloating">@color/material_dynamic_primary80</item>
        <item name="android:colorBackground">@color/material_dynamic_secondary80</item>
        <item name="colorOnBackground">@color/material_dynamic_primary20</item>

        <item name="colorError">@color/design_default_color_error</item>
        <item name="colorOnError">@color/material_dynamic_primary90</item>
    </style>

    <style name="Theme.BPRegister" parent="Base.Theme.BPRegister">
        <item name="android:navigationBarColor">@android:color/transparent</item>
    </style>

    <style name="Theme.BPRegister.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
